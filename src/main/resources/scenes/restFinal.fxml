<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.college.controller.RestFinalController"
            prefWidth="600" prefHeight="400" style="-fx-background-color: white;">

    <children>
        <VBox spacing="10" >
            <Label text="Restful Web Service Demo" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
            <HBox spacing="10">
                <Button text="User Hello" fx:id="btnUser" onAction="#callUserHello"/>
                <Button text="Manager Hello" fx:id="btnManager" onAction="#callManagerHello"/>
                <Button text="Admin Hello" fx:id="btnAdmin" onAction="#callAdminHello"/>
            </HBox>
            <TextArea fx:id="txtResponse" prefHeight="250" wrapText="true" editable="false"/>
        </VBox>
    </children>
</AnchorPane>
